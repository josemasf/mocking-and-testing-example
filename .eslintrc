{
  "env": {
    "browser": true
  },
  "extends": ["plugin:vue/vue3-essential", "@vue/typescript/recommended", "@vue/prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "no-debugger": "warn",
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "error"
  }
}
