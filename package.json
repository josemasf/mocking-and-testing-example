{
  "name": "mocking-and-testing-example",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dev:jss": "vite --mode jsserver",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:coverage": "vitest run --coverage --environment jsdom",
    "test:jss": "vitest --ui --environment jsdom --mode jsserver",
    "test:jss:coverage": "vitest run --coverage --environment jsdom  --mode jsserver",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "mocks": "json-server --watch db/customers.json --routes db/routes.json --port 3000"
  },
  "dependencies": {
    "json-server": "^0.17.1",
    "primeicons": "^6.0.1",
    "primevue": "^3.18.1",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/vue": "^6.6.1",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitest/coverage-c8": "^0.24.5",
    "@vitest/ui": "^0.24.5",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.1.0",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.1",
    "prettier": "^2.7.1",
    "vite": "^3.1.8",
    "vitest": "^0.24.3"
  }
}
