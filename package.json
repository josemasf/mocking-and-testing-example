{
  "name": "mocking-and-testing-example",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dev:jss": "vite --mode jsserver",
    "dev:msw": "vite --mode msw",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:coverage": "vitest run --coverage --environment jsdom",
    "lint:script": "eslint --ext .ts,vue --ignore-path .gitignore .",
    "test:jss": "vitest --ui --environment jsdom --mode jsserver",
    "test:jss:coverage": "vitest run --coverage --environment jsdom  --mode jsserver",
    "test:msw": "vitest --ui --environment jsdom --mode msw",
    "test:msw:coverage": "vitest run --coverage --environment jsdom  --mode msw",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "mocks": "json-server --watch db/customers.json --routes db/routes.json --port 3000"
  },
  "dependencies": {
    "json-server": "^0.17.1",
    "primeicons": "^6.0.1",
    "primevue": "^3.18.1",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/vue": "^6.6.1",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.68",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitest/coverage-c8": "^0.24.5",
    "@vitest/ui": "^0.24.5",
    "@vue/compiler-sfc": "^3.1.4",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "commitizen": "^4.2.4",
    "cz-emoji": "^1.3.1",
    "eslint": "^8.22.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^4.3.0",
    "jsdom": "^20.0.1",
    "lint-staged": "^10.5.0",
    "msw": "^0.48.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "~4.7.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.3",
    "vue-tsc": "^1.0.8"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
